.order-container{
  background-color: #F7F7F7;
  min-height: 100vh;
  background: #F7F7F7;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}
.MyNav{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  width: 100%;
  z-index: 9;
  border-bottom: 1px solid #F7F7F7;
}

.order-container .list{
  font-size: 34rpx;
  margin-top: 90rpx;
  box-sizing: border-box;
  padding-top: 60rpx;
  padding-bottom: 80rpx;
}

.order-container .item{
  position: relative;
  margin-bottom: 30rpx;
  background: #ffffff;
}
.order-container .item-footer{
  padding: 0 30rpx 20rpx;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}
.order-container .item-footer-text{
  color: #666;
  font-size: 28rpx;
  margin: 20rpx 0 40rpx;
}
.order-container .item-footer-text text{
  padding: 0 10rpx;
}
.order-container .item-footer-button{
  font-size: 28rpx;
  font-weight: 400;
  font-family: inherit;
  text-decoration: none;
  text-align: center;
  padding: 0 24rpx;
  margin: 0 10rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 8rpx;
}